{
    "$id": "https://lfgheroes.com/events/GroupSessionCompleted",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "GroupSessionCompleted",
    "type": "object",
    "properties": {
        "sessionId": {
            "type": "string",
            "description": "Globally unique ID for the group session"
        },
        "gameId": {
            "type": "string",
            "description": "Globally unique identifier for the game"
        },
        "playerId": {
            "type": "string",
            "description": "Originating player's unique ID"
        },    
        "note": {
            "type": "string",
            "description": "Optional notes for the end of session"
        },    
        "timestamp": {
            "type": "string",
            "description": "RFC 3339 timestamp"
        }
    },
    "required": [
        "sessionId",
        "gameId",
        "playerId",        
        "timestamp"        
    ]
}